<div class="contentpanel">





    <div class="panel panel-default">

        <div class="panel-body ">
            <h5 class="subtitle mb5">Modifier Passager</h5>







            <form class="form-horizontal form-bordered" action="" method="post">

                <input type="hidden" value=""/>

                <div class="form-group">
                    <label class="col-sm-3 control-label">Nom :</label>
                    <div class="col-sm-6">
                        <input type="text" id="action_nom" name="nom" value="{{ nom }}" required="required" class="form-control">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-3 control-label">Prenom :</label>
                    <div class="col-sm-6">
                        <input type="text" id="action_nom" name="prenom" value="{{ prenom }}" required="required" class="form-control">
                    </div>
                </div>


                <div class="form-group">
                    <label class="col-sm-3 control-label">Sexe :</label>
                    <div class="col-sm-6">
                        <!-- <input type="text" placeholder="Default Input" class="form-control" /> -->
                        <select id="action_sexe" name="sexe" class="form-control input-sm mb15">
                            <option
                                    {% if sexe=='F' %}
                                    selected="selected"
                                   {% endif %}
                                    value="F">Féminin</option>
                            <option                                     {% if sexe=='M' %}
                                selected="selected"
                            {% endif %}
                                    value="M">Masculin</option>
                        </select>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-3 control-label">Date de Naissance :</label>
                    <div class="col-sm-6">


                        <div class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>

                            <input type="text" id="action_dateNaissance" value="{{ dn }}" name="dateNaissance" required="required" class="form-control" placeholder="mm/dd/yyyy">

                        </div>



                    </div>
                </div>





                <div class="form-group">
                    <label class="col-sm-3 control-label">Mot de passe :</label>
                    <div class="col-sm-6">
                        <input type="text" id="action_nom" name="password" class="form-control">
                    </div>
                </div>






                <div class="row">
                    <div class="col-sm-6 col-sm-offset-3">
                        <input type="submit" value="Ajouter" class="btn btn-primary">&nbsp;
                        <input type="reset" value="Annuler" class="btn btn-default">
                    </div>
                </div>

            </form>
            <p><a href="{{ path('siteadmin_passager_lister') }}">Retour à la liste des passagers</a></p>


